class Solution {

    /**
     * @param Integer[] $nums
     * @param Integer $val
     * @return Integer
     */
    function removeElement(&$nums, $val) {
        $n = count($nums);$count = 0;
        for($i = 0;$i<$n;$i++){
            if($nums[$i]!=$val){
                $nums[$count++] = $nums[$i];
            }
        }
        return $count;
    }
}
